#!groovy
pipeline {
    agent {
        label 'stable && docker && linux && ansible'
    }
    stages {
        stage('Build Docker Images') {
            steps {
		unstash 'artifacts'
		sh 'ls -la'
		SH 'ls -la target'
                sh 'ansible-playbook aurea-central-jervis/ansible/jervis.yml -i inventories/staging -vvvv -e environment_name=qa'
            }
        }
    }
}
