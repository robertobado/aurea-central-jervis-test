#!groovy
pipeline {
    agent {
        label 'stable && docker && linux && ansible'
    }
    stages {
        stage('Build Docker Image') {
            steps {
                sh 'env | LC_ALL=C sort'
                unstash 'artifacts'
                unstash 'junit'
                sh 'find . -type f'
                sh 'pwd'
                sh 'tree'
            }
        }
    }
}
